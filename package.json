{
  "name": "advanced-panorama-viewer",
  "version": "0.1.0",
  "description": "Advanced Panorama Viewer - A professional panoramic image viewer application",
  "author": "FRIBOUZ",
  "private": true,
  "main": "desktop/main.js",
  "scripts": {
    "dev": "node scripts/utils/dev-with-projects-path.js",
    "desktop:dev": "concurrently \"npm run dev\" \"wait-on http://localhost:3000 && electron desktop/main.js\"",
    "build": "node scripts/build.js web",
    "desktop:build:unpack": "node scripts/build.js electron",
    "desktop:build:installer": "node scripts/build.js electron-installer",
    "start": "node .next/standalone/server.js",
    "lint": "next lint",
    "generate-config": "node scripts/node/generate-config.js",
    "clean": "node scripts/build.js clean",
    "clean:electron": "node scripts/electron/clear-electron-data.js",
    "test:credentials": "node scripts/utils/validate-credentials.js",
    "test": "cd . && jest --config=config/jest.config.js",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky install",
    "postinstall": "echo \"Build fixes are now integrated into the build process\""
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.52.0",
    "@types/archiver": "^6.0.3",
    "@types/formidable": "^3.4.5",
    "@types/uuid": "^10.0.0",
    "adm-zip": "^0.5.10",
    "app-builder-bin": "^5.0.0-alpha.12",
    "archiver": "^7.0.1",
    "axios": "^1.10.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "form-data": "^4.0.4",
    "formidable": "^3.5.4",
    "fs-extra": "^11.1.1",
    "helmet": "^7.1.0",
    "mime-types": "^2.1.35",
    "next": "^14.2.30",
    "node-fetch": "^3.3.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.3.8",
    "react-icons": "^5.5.0",
    "react-toastify": "^11.0.5",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^2",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.1.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^14.5.0",
    "@types/adm-zip": "^0.5.7",
    "@types/jest": "^29.5.14",
    "@types/node": "^20",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "electron": "^32.2.0",
    "electron-builder": "^26.0.12",
    "electron-builder-squirrel-windows": "^24.13.3",
    "electron-packager": "^17.1.2",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.6.0",
    "husky": "^8.0.3",
    "javascript-obfuscator": "^4.1.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "lint-staged": "^15.2.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "tailwindcss": "^4",
    "ts-prune": "^0.10.3",
    "typescript": "^5",
    "wait-on": "^8.0.3"
  }
}
